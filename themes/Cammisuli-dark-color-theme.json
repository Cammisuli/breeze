{
  "comment": "",
  "name": "Cammisuli Theme",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "parameter": {
      "fontStyle": "italic"
    },
    "variable.local": {
      "foreground": "#ECE3C0"
    },
    "variable.defaultLibrary": {
      "foreground": "#247BA0",
      "bold": true,
      "italic": true
    }
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#2CC02C"
      }
    },

    {
      "name": "annotations",
      "scope": ["meta meta.type.annotation"],
      "settings": {
        "foreground": "#FFEEAD"
      }
    },
    {
      "name": "support class",
      "scope": ["support.class"],
      "settings": {
        "foreground": "#9289FE"
      }
    },
    {
      "name": "annotation parameters",
      "scope": [
        "meta meta.type.annotation meta.type.parameters",
        "meta new.expr meta.type.parameters entity.name.type"
      ],
      "settings": {
        "foreground": "#FFF"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#FFCB5B"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#FFEEAD"
      }
    },
    {
      "name": "tagged template",
      "scope": [
        "string.js.taggedTemplate",
        "string.js.taggedTemplate punctuation.definition.string.template.end.js",
        "string.js.taggedTemplate string.unquoted.html"
      ],
      "settings": {
        "foreground": "#FFF"
      }
    },
    // {
    //   "name": "Function, Special Method, Block Level",
    //   "scope": [
    //     "meta.function-call",
    //     "support.function",
    //     "keyword.other.special-method",
    //     "meta.block-level"
    //   ],
    //   "settings": {
    //     "foreground": "#9289FE"
    //   }
    // },
    {
      "name": "Object properties",
      "scope": [
        "variable.other.object.property",
        "variable.object.property",
        "variable",
        "meta.type.parameters entity.name.type",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#FFF"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage",
        "keyword.operator.new",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#1287DB"
      }
    },
    {
      "name": "Keyword operation",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "keyword control flow, constant language",
      "scope": ["keyword.control.conditional", "constant.language"],
      "settings": {
        "foreground": "#9289FE"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Separator",
      "scope": "meta.separator",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Diff insertion",
      "scope": ["markup.inserted.diff", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#718c00"
      }
    },
    {
      "name": "Diff deletion",
      "scope": ["markup.deleted.diff", "meta.diff.header.from-file"],
      "settings": {
        "foreground": "#c82829"
      }
    },
    {
      "name": "Diff header",
      "scope": ["meta.diff.header.from-file", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Diff range",
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#3e999f"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#FFC58F"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#BBDAFF"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#FFC58F"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF9DA4"
      }
    },
    {
      "name": "Decorator",
      "scope": ["meta.decorator entity.name.function"],
      "settings": {
        "foreground": "#E5BA8F"
      }
    },
    {
      "name": "Class, Interface name",
      "scope": ["entity.name.type.class", "entity.name.type.interface"],
      "settings": {
        "foreground": "#E2EDFF"
      }
    },
    {
      "name": "Global, this",
      "scope": [
        "variable.language.this",
        "support.variable.dom",
        "support.class"
      ],
      "settings": {
        "foreground": "#40A893"
      }
    },
    {
      "name": "JSON property",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#E2EDFF"
      }
    },
    {
      "name": "CSS variable",
      "scope": "source.css variable",
      "settings": {
        "foreground": "#9289FE"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "source.css.scss meta.property-value.scss",
        "source.css meta.property-value.css",
        "source.css keyword.other.unit"
      ],
      "settings": {
        "foreground": "#ffeead"
      }
    },
    {
      "name": "italics",
      "scope": [
        "comment",
        "entity.other.inherited-class",
        "entity.other.attribute-name",
        "meta.class meta.method.declaration meta.var.expr storage.type.js",
        "meta.delimiter.period",
        "meta.selector",
        "entity.name.tag.doctype",
        "meta.tag.sgml.doctype",
        "italic",
        "quote",
        "source.elixir .punctuation.binary.elixir",
        "meta.tag.sgml.doctype.html",
        "markup.italic.markdown",
        "variable.language.this",
        "source.css.scss meta.property-value.scss",
        "source.css meta.property-value.css",
        "storage",
        "keyword.operator.new",
        "keyword.control.conditional",
        "constant.language",
        "meta.type.annotation",
        "meta.return.type"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ],
  "colors": {
    "activityBar.background": "#252931",
    "activityBar.foreground": "#f1f1f1",
    "activityBarBadge.background": "#1287DB",
    "editor.background": "#1C2026",
    "editor.foreground": "#f1f1f1",
    "editorGroupHeader.tabsBackground": "#1c2026",
    "focusBorder": "#444444",
    "list.hoverBackground": "#444444",
    "list.activeSelectionBackground": "#fff1",
    "list.inactiveSelectionBackground": "#fff1",
    "list.inactiveSelectionForeground": "#f1f1f1",
    "panel.border": "#21252c",
    "sideBar.background": "#21252c",
    "sideBar.border": "#0000",
    "sideBarSectionHeader.background": "#252931",
    "sideBarSectionHeader.foreground": "#f1f1f1",
    "scrollbar.shadow": "#1C2026",
    "statusBar.background": "#1C2026",
    "statusBar.border": "#1C2026",
    "tab.border": "#1c2026",
    "tab.inactiveBackground": "#1c2026",
    "titleBar.border": "#0000",
    "tree.indentGuidesStroke": "#fff1",
    "editorLineNumber.foreground": "#3d4853",
    "editorLineNumber.activeForeground": "#f1f1f1",
    "editorIndentGuide.background": "#3d4853",
    "editorIndentGuide.activeBackground": "#e6e6e6",
    "breadcrumbPicker.background": "#21252c",
    "editor.lineHighlightBorder": "#1C2026",
    "editorWidget.background": "#1C2026",
    "editorWidget.border": "#1C2026",
    "input.background": "#fff1",
    "dropdown.background": "#fff1",
    "button.background": "#1287DB"
  }
}
