{
    "comment": "",
    "name": "Cammisuli Theme",
    "tokenColors": [
        {
            "name": "Global settings",
            "settings": {
                "background": "#333333",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#2CC02C"
            }
        },
        {
            "name": "Variable, String Link, Regular Expression, Tag Name",
            "scope": [
                "variable",
                "support.other.variable",
                "string.other.link",
                "string.regexp",
                "entity.name.tag",
                "entity.other.attribute-name",
                "meta.tag",
                "declaration.tag",
                "punctuation.definition.template-expression",
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#E2EDFF",
                "fontStyle": "none"
            }
        },
        {
            "name": "annotations, support class",
            "scope": ["meta meta.type.annotation", "support.class"],
            "settings": {
                "fontStyle": "",
                "foreground": "#FFEEAD"
            }
        },
        {
            "name": "annotation parameters",
            "scope": [
                "meta meta.type.annotation meta.type.parameters",
                "meta new.expr meta.type.parameters entity.name.type"
            ],
            "settings": {
                "foreground": "#FFF",
                "fontStyle": "none"
            }
        },
        {
            "name": "String",
            "scope": ["string"],
            "settings": {
                "foreground": "#FFCB5B"
            }
        },
        {
            "name": "Inherited Class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#FFEEAD"
            }
        },
        {
            "name": "tagged template",
            "scope": [
                "string.js.taggedTemplate",
                "string.js.taggedTemplate punctuation.definition.string.template.end.js",
                "string.js.taggedTemplate string.unquoted.html"
            ],
            "settings": {
                "fontStyle": "none",
                "foreground": "#FFF"
            }
        },
        {
            "name": "Function, Special Method, Block Level",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function",
                "keyword.other.special-method",
                "meta.block-level"
            ],
            "settings": {
                "foreground": "#cacaca"
            }
        },
        {
            "name": "Object properties",
            "scope": ["variable.other.object.property", "variable.object.property"],
            "settings": {
                "foreground": "#FFF"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": ["keyword", "storage", "entity.name.tag.css"],
            "settings": {
                "fontStyle": "",
                "foreground": "#58A8FD"
            }
        },
        {
            "name": "Keyword operation",
            "scope": ["keyword.operator"],
            "settings": {
                "fontStyle": "none",
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "keyword control flow, constant language",
            "scope": ["keyword.control.conditional", "constant.language"],
            "settings": {
                "foreground": "#9289FE"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "background": "#F99DA5",
                "fontStyle": "",
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Separator",
            "scope": "meta.separator",
            "settings": {
                "background": "#BBDAFE",
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "background": "#EBBBFF",
                "fontStyle": "",
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Diff insertion",
            "scope": ["markup.inserted.diff", "meta.diff.header.to-file"],
            "settings": {
                "foreground": "#718c00"
            }
        },
        {
            "name": "Diff deletion",
            "scope": ["markup.deleted.diff", "meta.diff.header.from-file"],
            "settings": {
                "foreground": "#c82829"
            }
        },
        {
            "name": "Diff header",
            "scope": ["meta.diff.header.from-file", "meta.diff.header.to-file"],
            "settings": {
                "foreground": "#FFFFFF",
                "background": "#4271ae"
            }
        },
        {
            "name": "Diff range",
            "scope": "meta.diff.range",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#3e999f"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#FFC58F"
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#BBDAFF"
            }
        },
        {
            "name": "Markup Styling",
            "scope": ["markup.bold", "markup.italic"],
            "settings": {
                "foreground": "#FFC58F"
            }
        },
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "",
                "foreground": "#FF9DA4"
            }
        },
        {
            "name": "Decorator",
            "scope": [
                "meta.decorator entity.name.function",
                "meta.decorator punctuation.decorator",
                "meta.decorator meta.brace.round"
            ],
            "settings": {
                "foreground": "#E5BA8F"
            }
        },
        {
            "name": "Class, Interface name",
            "scope": ["entity.name.type.class", "entity.name.type.interface"],
            "settings": {
                "foreground": "#E2EDFF"
            }
        },
        {
            "name": "new keyword",
            "scope": ["keyword.operator.new.ts"],
            "settings": {
                "foreground": "#58A8FD"
            }
        },
        {
            "name": "new class",
            "scope": ["new.expr entity.name.type"],
            "settings": {
                "foreground": {}
            }
        },
        {
            "name": "Global, this",
            "scope": ["variable.language.this", "support.variable.dom"],
            "settings": {
                "foreground": "#40A893"
            }
        },
        {
            "name": "JSON property",
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "#E2EDFF"
            }
        },
        {
            "name": "HTML Attribute",
            "scope": "entity.other.attribute-name.html",
            "settings": {
                "foreground": "#52A0FD",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS variable",
            "scope": "source.css variable",
            "settings": {
                "foreground": "#9289FE"
            }
        },
        {
            "name": "CSS property value",
            "scope": [
                "source.css.scss meta.property-value.scss",
                "source.css meta.property-value.css",
                "source.css keyword.other.unit"
            ],
            "settings": {
                "foreground": "#ffeead"
            }
        },
        {
            "name": "italics",
            "scope": [
                "comment",
                "entity.other.inherited-class",
                "keyword",
                "entity.name.function",
                "string.quoted",
                "support.function",
                "punctuation.accessor",
                "meta.block.js",
                "entity.other.attribute-name",
                "meta.class meta.method.declaration meta.var.expr storage.type.js",
                "keyword.operator.relational",
                "meta.delimiter.period",
                "meta.selector",
                "entity.name.tag.doctype",
                "meta.tag.sgml.doctype",
                "italic",
                "quote",
                "source.elixir .punctuation.binary.elixir",
                "meta.tag.sgml.doctype.html",
                "markup.italic.markdown",
                "variable.language.this",
                "new.expr entity.name.type",
                "source.css.scss meta.property-value.scss",
                "source.css meta.property-value.css"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        }
    ],
    "colors": {
        "focusBorder": "#58a8fd",
        "foreground": "#ffffff",
        "dropdown.background": "#515151",
        "input.background": "#515151",
        "inputOption.activeBorder": "#58a8fd",
        "list.activeSelectionBackground": "#58a8fd80",
        "list.dropBackground": "#58a8fd80",
        "list.focusBackground": "#58a8fd80",
        "list.hoverBackground": "#FFFFFF1a",
        "list.inactiveSelectionBackground": "#FFFFFF33",
        "activityBar.background": "#474747",
        "activityBar.dropBackground": "#58a8fd80",
        "activityBarBadge.background": "#58a8fd",
        "sideBar.background": "#3d3d3d",
        "sideBarSectionHeader.background": "#474747",
        "editorGroup.dropBackground": "#58a8fd80",
        "editorGroupHeader.tabsBackground": "#3d3d3d",
        "tab.border": "#00000033",
        "tab.activeBorder": "#58a8fd",
        "tab.inactiveBackground": "#474747",
        "editor.background": "#333333",
        "editor.foreground": "#ffffff",
        "editorLineNumber.foreground": "#FFFFFF4d",
        "editorLineNumber.activeForeground": "#58a8fd",
        "editor.lineHighlightBorder": "#FFFFFF1a",
        "editor.rangeHighlightBackground": "#FFFFFF0d",
        "editorWidget.background": "#3d3d3d",
        "editorHoverWidget.background": "#3d3d3d",
        "editorMarkerNavigation.background": "#3d3d3d",
        "peekView.border": "#58a8fd",
        "peekViewEditor.background": "#282828",
        "peekViewResult.background": "#3d3d3d",
        "peekViewTitle.background": "#333333",
        "panel.background": "#3d3d3d",
        "panel.border": "#FFFFFF1a",
        "panelTitle.activeBorder": "#ffffff80",
        "panelTitle.inactiveForeground": "#ffffff80",
        "statusBar.background": "#282828",
        "statusBar.debuggingBackground": "#58a8fd",
        "statusBar.noFolderBackground": "#282828",
        "statusBarItem.activeBackground": "#58a8fd80",
        "statusBarItem.hoverBackground": "#FFFFFF1a",
        "titleBar.activeBackground": "#282828",
        "pickerGroup.border": "#FFFFFF1a",
        "debugToolBar.background": "#474747",
        "selection.background": "#58a8fd"
    }
}
