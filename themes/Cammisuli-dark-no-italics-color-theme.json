{
  "comment": "",
  "name": "Cammisuli Theme",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable.local": {
      "foreground": "#ECE3C0"
    },
    "variable.defaultLibrary": {
      "foreground": "#247BA0",
      "italic": true
    },
    "*.readonly": "#ECE3C0",
    "member": "#f1f1f1",
    "enumMember": {
      "foreground": "#9289FE",
      "italic": true
    },
    "*.static": {
      "foreground": "#9289FE",
      "italic": true
    },
    "number": "#FF7BA6"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#2CC02C"
      }
    },
    {
      "name": "numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FF7BA6"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#E5D48B"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#f1f1f1"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#FFEEAD"
      }
    },
    {
      "name": "Keyword, Storage, primitive, constants",
      "scope": [
        "keyword",
        "storage",
        "keyword.operator.new",
        "entity.other.attribute-name",
        "support.type.primitive",
        "constant.language"
      ],
      "settings": {
        "foreground": "#1287DB"
      }
    },
    {
      "name": "Keyword operation",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#F1f1f1"
      }
    },
    {
      "name": "support class",
      "scope": ["support.class.builtin"],
      "settings": {
        "foreground": "#9289FE"
      }
    },
    {
      "name": "keyword control flow ",
      "scope": ["keyword.control.conditional"],
      "settings": {
        "foreground": "#9289FE"
      }
    },
    {
      "name": "Diff insertion",
      "scope": ["markup.inserted.diff", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#718c00"
      }
    },
    {
      "name": "Diff deletion",
      "scope": ["markup.deleted.diff", "meta.diff.header.from-file"],
      "settings": {
        "foreground": "#c82829"
      }
    },
    {
      "name": "Diff header",
      "scope": ["meta.diff.header.from-file", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Diff range",
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#3e999f"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#FFC58F"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#BBDAFF"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#FFC58F"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF9DA4"
      }
    },
    {
      "name": "Class, Interface name",
      "scope": ["meta.class", "interface"],
      "settings": {
        "foreground": "#E2EDFF"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "meta.decorator entity.name.function",
        "punctuation.decorator",
        "meta.attribute"
      ],
      "settings": {
        "foreground": "#E5BA8F"
      }
    },
    {
      "name": "function calls",
      "scope": [
        "entity.name.function meta.function-call",
        "entity.name.function.tagged-template"
      ],
      "settings": {
        "foreground": "#f1f1f1"
      }
    },
    {
      "name": "Global, this",
      "scope": ["variable.language.this", "support.variable.dom"],
      "settings": {
        "foreground": "#40A893"
      }
    },
    {
      "name": "JSON property",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#E2EDFF"
      }
    },
    {
      "name": "CSS variable",
      "scope": "source.css variable",
      "settings": {
        "foreground": "#9289FE"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "source.css.scss meta.property-value.scss",
        "source.css meta.property-value.css",
        "source.css keyword.other.unit"
      ],
      "settings": {
        "foreground": "#ffeead"
      }
    }
  ],
  "colors": {
    "activityBar.background": "#252931",
    "activityBar.foreground": "#f1f1f1",
    "activityBarBadge.background": "#1287DB",
    "editor.background": "#1C2026",
    "editor.foreground": "#f1f1f1",
    "editorGroupHeader.tabsBackground": "#1c2026",
    "focusBorder": "#3d4853",
    "list.hoverBackground": "#3d4853",
    "list.activeSelectionBackground": "#fff1",
    "list.inactiveSelectionBackground": "#fff1",
    "list.inactiveSelectionForeground": "#f1f1f1",
    "panel.border": "#21252c",
    "sideBar.background": "#21252c",
    "sideBar.border": "#0000",
    "sideBarSectionHeader.background": "#252931",
    "sideBarSectionHeader.foreground": "#f1f1f1",
    "scrollbar.shadow": "#1C2026",
    "statusBar.background": "#1C2026",
    "statusBar.border": "#1C2026",
    "tab.border": "#1c2026",
    "tab.inactiveBackground": "#1c2026",
    "titleBar.border": "#0000",
    "tree.indentGuidesStroke": "#fff1",
    "editorLineNumber.foreground": "#3d4853",
    "editorLineNumber.activeForeground": "#f1f1f1",
    "editorIndentGuide.background": "#3d4853",
    "editorIndentGuide.activeBackground": "#e6e6e6",
    "breadcrumbPicker.background": "#21252c",
    "editor.lineHighlightBorder": "#1C2026",
    "editorWidget.background": "#1C2026",
    "editorWidget.border": "#3d4853",
    "input.background": "#fff1",
    "dropdown.background": "#fff1",
    "button.background": "#1287DB"
  }
}
