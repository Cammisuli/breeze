{
  "comment": "",
  "name": "Cammisuli Theme",
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#2CC02C"
      }
    },
    {
      "name": "Variable, String Link, Regular Expression, Tag Name",
      "scope": [
        "support.other.variable",
        "string.other.link",
        "string.regexp",
        "entity.name.tag",
        "entity.other.attribute-name",
        "meta.tag",
        "declaration.tag",
        "punctuation.definition.template-expression",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#E2EDFF"
      }
    },
    {
      "name": "annotations",
      "scope": ["meta meta.type.annotation"],
      "settings": {
        "foreground": "#FFEEAD"
      }
    },
    {
      "name": "support class",
      "scope": ["support.class"],
      "settings": {
        "foreground": "#9289FE"
      }
    },
    {
      "name": "annotation parameters",
      "scope": [
        "meta meta.type.annotation meta.type.parameters",
        "meta new.expr meta.type.parameters entity.name.type"
      ],
      "settings": {
        "foreground": "#FFF"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#FFCB5B"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#FFEEAD"
      }
    },
    {
      "name": "tagged template",
      "scope": [
        "string.js.taggedTemplate",
        "string.js.taggedTemplate punctuation.definition.string.template.end.js",
        "string.js.taggedTemplate string.unquoted.html"
      ],
      "settings": {
        "foreground": "#FFF"
      }
    },
    {
      "name": "Function, Special Method, Block Level",
      "scope": [
        "meta.function-call",
        "support.function",
        "keyword.other.special-method",
        "meta.block-level"
      ],
      "settings": {
        "foreground": "#cacaca"
      }
    },
    {
      "name": "Object properties",
      "scope": [
        "variable.other.object.property",
        "variable.object.property",
        "variable",
        "meta.type.parameters entity.name.type"
      ],
      "settings": {
        "foreground": "#FFF"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage",
        "source meta.type.annotation.ts support.type.primitive.ts",
        "entity.name.tag.css",
        "meta.return.type"
      ],
      "settings": {
        "foreground": "#58A8FD"
      }
    },
    {
      "name": "Keyword operation",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "keyword control flow, constant language",
      "scope": ["keyword.control.conditional", "constant.language"],
      "settings": {
        "foreground": "#9289FE"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Separator",
      "scope": "meta.separator",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Diff insertion",
      "scope": ["markup.inserted.diff", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#718c00"
      }
    },
    {
      "name": "Diff deletion",
      "scope": ["markup.deleted.diff", "meta.diff.header.from-file"],
      "settings": {
        "foreground": "#c82829"
      }
    },
    {
      "name": "Diff header",
      "scope": ["meta.diff.header.from-file", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Diff range",
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#3e999f"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#FFC58F"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#BBDAFF"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#FFC58F"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF9DA4"
      }
    },
    {
      "name": "Decorator",
      "scope": ["meta.decorator entity.name.function"],
      "settings": {
        "foreground": "#E5BA8F"
      }
    },
    {
      "name": "Class, Interface name",
      "scope": ["entity.name.type.class", "entity.name.type.interface"],
      "settings": {
        "foreground": "#E2EDFF"
      }
    },
    {
      "name": "new keyword",
      "scope": ["keyword.operator.new"],
      "settings": {
        "foreground": "#58A8FD"
      }
    },
    {
      "name": "Global, this",
      "scope": ["variable.language.this", "support.variable.dom"],
      "settings": {
        "foreground": "#40A893"
      }
    },
    {
      "name": "JSON property",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#E2EDFF"
      }
    },
    {
      "name": "HTML Attribute",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#52A0FD"
      }
    },
    {
      "name": "CSS variable",
      "scope": "source.css variable",
      "settings": {
        "foreground": "#9289FE"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "source.css.scss meta.property-value.scss",
        "source.css meta.property-value.css",
        "source.css keyword.other.unit"
      ],
      "settings": {
        "foreground": "#ffeead"
      }
    }
  ],
  "colors": {
    "editor.background": "#333333",
    "editor.foreground": "#f1f1f1",
    "sideBar.background": "#333333",
    "list.hoverBackground": "#444444",
    "sideBarSectionHeader.background": "#333333",
    "sideBarSectionHeader.foreground": "#f1f1f1",
    "statusBar.background": "#58a8fd",
    "activityBarBadge.background": "#58a8fd",
    "activityBar.background": "#333333",
    "activityBar.foreground": "#f1f1f1",
    "tab.inactiveBackground": "#444444",
    "tab.border": "#444444",
    "editorGroupHeader.tabsBackground": "#444444",
    "panel.border": "#333333",
    "focusBorder": "#444444"
  }
}
