{
  "comment": "",
  "name": "Cammisuli Theme",
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#2CC02C"
      }
    },
    {
      "name": "Variable, String Link, Regular Expression, Tag Name",
      "scope": [
        "support.other.variable",
        "string.other.link",
        "string.regexp",
        "entity.name.tag",
        "entity.other.attribute-name",
        "meta.tag",
        "declaration.tag",
        "punctuation.definition.template-expression",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#3985ff"
      }
    },
    {
      "name": "annotations",
      "scope": ["meta meta.type.annotation"],
      "settings": {
        "foreground": "#caa200"
      }
    },
    {
      "name": "support class",
      "scope": ["support.class"],
      "settings": {
        "foreground": "#9289FE"
      }
    },
    {
      "name": "annotation parameters",
      "scope": [
        "meta meta.type.annotation meta.type.parameters",
        "meta new.expr meta.type.parameters entity.name.type"
      ],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#df8c1f"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#caa200"
      }
    },
    {
      "name": "tagged template",
      "scope": [
        "string.js.taggedTemplate",
        "string.js.taggedTemplate punctuation.definition.string.template.end.js",
        "string.js.taggedTemplate string.unquoted.html"
      ],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "Function, Special Method, Block Level",
      "scope": [
        "meta.function-call",
        "support.function",
        "keyword.other.special-method",
        "meta.block-level"
      ],
      "settings": {
        "foreground": "#002366"
      }
    },
    {
      "name": "Object properties",
      "scope": [
        "variable.other.object.property",
        "variable.object.property",
        "variable",
        "meta.type.parameters entity.name.type"
      ],
      "settings": {
        "foreground": "#3d405b"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage",
        "source meta.type.annotation.ts support.type.primitive.ts",
        "entity.name.tag.css",
        "meta.return.type"
      ],
      "settings": {
        "foreground": "#4169e1"
      }
    },
    {
      "name": "Keyword operation",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "keyword control flow, constant language",
      "scope": ["keyword.control.conditional", "constant.language"],
      "settings": {
        "foreground": "#9289FE"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "",
        "foreground": "#333333"
      }
    },
    {
      "name": "Separator",
      "scope": "meta.separator",
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "",
        "foreground": "#333333"
      }
    },
    {
      "name": "Diff insertion",
      "scope": ["markup.inserted.diff", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#718c00"
      }
    },
    {
      "name": "Diff deletion",
      "scope": ["markup.deleted.diff", "meta.diff.header.from-file"],
      "settings": {
        "foreground": "#c82829"
      }
    },
    {
      "name": "Diff header",
      "scope": ["meta.diff.header.from-file", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "Diff range",
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#3e999f"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#FFC58F"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#BBDAFF"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#FFC58F"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF9DA4"
      }
    },
    {
      "name": "Decorator",
      "scope": ["meta.decorator entity.name.function"],
      "settings": {
        "foreground": "#ad8c6b"
      }
    },
    {
      "name": "Class, Interface name",
      "scope": ["entity.name.type.class", "entity.name.type.interface"],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "new keyword",
      "scope": ["keyword.operator.new"],
      "settings": {
        "foreground": "#4169e1"
      }
    },
    {
      "name": "Global, this",
      "scope": ["variable.language.this", "support.variable.dom"],
      "settings": {
        "foreground": "#0dc49f"
      }
    },
    {
      "name": "JSON property",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#3985ff"
      }
    },
    {
      "name": "HTML Attribute",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#52A0FD"
      }
    },
    {
      "name": "CSS variable",
      "scope": "source.css variable",
      "settings": {
        "foreground": "#9289FE"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "source.css.scss meta.property-value.scss",
        "source.css meta.property-value.css",
        "source.css keyword.other.unit"
      ],
      "settings": {
        "foreground": "#caa200"
      }
    },
    {
      "name": "italics",
      "scope": [
        "comment",
        "entity.other.inherited-class",
        "keyword",
        "support.function",
        "punctuation.accessor",
        "entity.other.attribute-name",
        "meta.class meta.method.declaration meta.var.expr storage.type.js",
        "keyword.operator.relational",
        "meta.delimiter.period",
        "meta.selector",
        "entity.name.tag.doctype",
        "meta.tag.sgml.doctype",
        "italic",
        "quote",
        "source.elixir .punctuation.binary.elixir",
        "meta.tag.sgml.doctype.html",
        "markup.italic.markdown",
        "variable.language.this",
        "source.css.scss meta.property-value.scss",
        "source.css meta.property-value.css",
        "storage",
        "keyword.operator.new",
        "constant.language",
        "meta.type.annotation",
        "meta.return.type"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ],
  "colors": {
    "activityBar.background": "#d8dcdf",
    "activityBar.foreground": "#616a75",
    "activityBarBadge.background": "#4169e1",
    "editor.background": "#f1f1f1",
    "editor.foreground": "#333333",
    "editorGroupHeader.tabsBackground": "#e6e6e6",
    "editorGroupHeader.tabsBorder": "#eef0f1",
    "list.activeSelectionBackground": "#cdd1d6",
    "list.activeSelectionForeground": "#616a75",
    "list.inactiveSelectionForeground": "#616a75",
    "list.inactiveSelectionBackground": "#cdd1d6",
    "list.warningForeground": "#fa8d3e",
    "panel.border": "#ebebeb",
    "sideBar.background": "#e3e6e8",
    "sideBar.border": "#0000",
    "sideBar.foreground": "#7f8994",
    "sideBarSectionHeader.background": "#d8dcdf",
    "sideBarSectionHeader.foreground": "#333333",
    "statusBar.background": "#d8dcdf",
    "statusBar.border": "#0000",
    "statusBar.foreground": "#616a75",
    "tab.border": "#e6e6e6",
    "tab.inactiveBackground": "#e6e6e6",
    "tab.inactiveForeground": "#c5c9ce",
    "titleBar.activeBackground": "#cdd1d6",
    "titleBar.activeForeground": "#7f8994",
    "titleBar.border": "#0000",
    "tree.indentGuidesStroke": "#0001"
  }
}
